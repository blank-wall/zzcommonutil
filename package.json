{
  "name": "zzcommonutil",
  "displayName": "zzCommonUtil",
  "description": "通用工具助手，可一键生成脚本文件/图片转为base64...",
  "publisher": "guotianquan",
  "icon": "logo.png",
  "version": "0.6.7",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:zzcommonUtil",
    "onCommand:zzcommonModule",
    "onCommand:zzcommonBase64"
  ],
  "main": "./src/index.js",
  "contributes": {
    "commands": [
      {
        "command": "zzcommonUtil",
        "title": "zz: 生成通用工具文件"
      },
      {
        "command": "zzcommonModule",
        "title": "zz: 生成通用模板文件"
      },
      {
        "command": "zzcommonBase64",
        "title": "zz: 图片转base64"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "zzcommonUtil",
          "group": "navigation"
        },
        {
          "command": "zzcommonModule",
          "group": "navigation"
        }
      ],
      "editor/context": [
        {
          "when": "editorFocus",
          "command": "zzcommonBase64",
          "group": "navigation"
        }
      ]
    },
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/zzCommonFunc.json"
      },
      {
        "language": "javascriptreact",
        "path": "./snippets/zzCommonFunc.json"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "yarn run lint",
    "test": "node ./test/runTest.js",
    "package": "vsce package",
    "push": "vsce publish"
  },
  "devDependencies": {
    "@types/vscode": "^1.70.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4",
    "@vscode/test-electron": "^2.1.5"
  },
  "dependencies": {
    "yarn": "^1.22.19"
  }
}